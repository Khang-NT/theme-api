{
  "name": "gitbook-plugin-theme-api",
  "description": "Theme for using GitBook as an API documentation",
  "version": "1.1.3",
  "engines": {
    "gitbook": ">=3.0.4"
  },
  "dependencies": {
    "cheerio": "0.20.0",
    "gitbook-plugin-search": ">=2.0.0",
    "lodash": "4.12.0",
    "q": "1.4.1",
    "q-plus": "0.0.8"
  },
  "scripts": {
    "preinstall": "./src/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Khang-NT/theme-api.git"
  },
  "author": "FriendCode Inc. <contact@gitbook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GitbookIO/theme-api/issues"
  },
  "contributors": [
    {
      "name": "Johan Preynat",
      "email": "johan@gitbook.com"
    }
  ],
  "gitbook": {
    "properties": {
      "languages": {
        "type": "array",
        "description": "List of languages code snippets",
        "default": []
      },
      "split": {
        "type": "boolean",
        "description": "Initial layout for this documentation",
        "default": true
      },
      "enableSplitButton": {
        "type": "boolean",
        "description": "Toggle visibility split code button",
        "default": true
      },
      "theme": {
        "type": "string",
        "default": "light",
        "title": "Default theme",
        "enum": [
          "light",
          "dark"
        ]
      }
    }
  }
}
